		function fnPrint()
		{
			printBtn.style.visibility="hidden";
			window.print();	
			printBtn.style.visibility="visible";
		}
		 var transcriptText=window.top.opener.parent.transcriptString;
		 var CourseNameText=window.top.opener.parent.CourseNameString;
			document.write('<div  id="printBtn" align="right"><input type="button" value="Print" onclick="fnPrint()" /></div>');
			document.write("<h1>" + CourseNameText + "</h1>")
			document.writeln('<br>');
			document.write("<h2>"+transcriptText+"</h2>")
