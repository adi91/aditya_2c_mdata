	function fromas(str){

    var win = "popup";    
    var winWidth=1012;
    var winHeight=650;
   // var screenWidth=screen.width/2
   // var screenHeight=screen.height/2
    var xPos=0//screenWidth-(winWidth/2)
    var yPos=0//screenHeight-(winHeight/2)
    var option = "width="+winWidth+",height="+winHeight+",fullscreen=false,toolbar=no,titlebar=0,status=0,location=0,scrollbars=no, top="+yPos+",left="+xPos+"";
    //var option = "width="+winWidth+",height="+winHeight+", fullscreen = no,status = yes, scrollbars=no, top="+yPos+",left="+xPos+"";
	
   newWin =  window.open(str,"Pop",option);
   newWin.focus()
 }
			
