<script language ="JavaScript">	
function fnClose(){
win = this	
	for(x=0;x<=7;x++)
	{
		if((win.parent != null) && (win.parent != win))
		{
			win = win.parent
		}
		else
		{
			break;
		}
		
	}
	//alert (win.opener.name)
	//win.opener.location.reload(true)
	win.close()
}
function fnpopup(){

    var win = "popup";    
    var winWidth=1012;
    var winHeight=709;
   // var screenWidth=screen.width/2
   // var screenHeight=screen.height/2
    var xPos=0//screenWidth-(winWidth/2)
    var yPos=0//screenHeight-(winHeight/2)
    var option = "width="+winWidth+",height="+winHeight+",fullscreen=yes,toolbar=no,titlebar=0,status=0,location=0,scrollbars=no, top="+yPos+",left="+xPos+"";
    //var option = "width="+winWidth+",height="+winHeight+", fullscreen = no,status = yes, scrollbars=no, top="+yPos+",left="+xPos+"";
	
   newWin =  window.open("./shellLoader.html","course",option);
   newWin.focus()
 }

 
 
</script>

<body bgcolor="#ffffff" onLoad="fnpopup()">
