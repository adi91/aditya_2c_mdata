var txt4 = "<b>Scenario D - Responding to Criticism from a Customer</b><br />Instructions: Write the note to your supervisor about the customer's complaint. Once you are finished, click Done to save your answer. You will then have the opportunity to revise your work before submitting it to your journal."
var TextEntry2 = player.GetVar("TextEntry2");
var TextEntry4 = player.GetVar("TextEntry4");
var TextEntry6 = player.GetVar("TextEntry6");
var TextEntry8 = player.GetVar("TextEntry8");


//alert(player.GetVar("TextEntry3")+"  $$  "+player.GetVar("TextEntry1"))


var doc;
makeFrame()

function makeFrame() {

   ifrm = document.createElement("IFRAME");
   ifrm.setAttribute("id", "iFramePrint"); 
   ifrm.setAttribute("name", "iFramePrint"); 
   ifrm.style.width = 640+"px"; 
   ifrm.style.height = 10+"px"; 
   ifrm.style.left = 200+"px";
   ifrm.style.marginTop =-100+"px";
	 
    document.body.appendChild(ifrm); 
 	
	 if(ifrm.contentDocument) 
	 { 
		doc = ifrm.contentDocument; 
	 } 
	else 
	{
		doc = ifrm.contentWindow.document; 
	}

var inter = window.setInterval(function() {
    if(doc.readyState == "complete") {
      window.clearInterval(inter);
      //do stuff
	  doc.body.innerHTML = txt1 + "<br/>" + "<br/>" + TextEntry2 + "<br/>" + "<br/>"+ txt2+ "<br/>"   + "<br/>" + TextEntry4 + "<br/>"  + "<br/>" +  txt3 + "<br/>"   + "<br/>" + TextEntry6 + "<br/>" + "<br/>" + txt4 + "<br/>"   +"<br/>" + TextEntry8;
            window.frames["iFramePrint"].focus();
	window.frames["iFramePrint"].print();
    }
},100);


}
}

